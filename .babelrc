{
  "presets": [ "react", "es2015", "stage-0" ],
  "plugins": [ "add-module-exports", "transform-class-properties" ],
  "env": {
    "development": {
      "plugins": [
        [ "react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": [ "react" ],
            "locals": [ "module" ]
          }]
        }]
      ]
    }
  }
}
